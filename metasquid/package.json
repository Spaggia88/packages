{
  "name": "@kodadot1/metasquid",
  "version": "1.0.0-rc.0",
  "description": "Utils for the @subsquid indexers",
  "repository": "@kodadot1/metasquid",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./consolidator": {
      "types": "./dist/consolidator.d.ts",
      "import": "./dist/consolidator.mjs",
      "require": "./dist/consolidator.cjs"
    },
    "./entity": {
      "types": "./dist/entity.d.ts",
      "import": "./dist/entity.mjs",
      "require": "./dist/entity.cjs"
    },
    "./is": {
      "types": "./dist/is.d.ts",
      "import": "./dist/is.mjs",
      "require": "./dist/is.cjs"
    },
    "./logger": {
      "types": "./dist/logger.d.ts",
      "import": "./dist/logger.mjs",
      "require": "./dist/logger.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "lint": "eslint --ext .ts,.js,.mjs,.cjs .",
    "prepack": "unbuild",
    "release": "pnpm test && changelogen && git push --follow-tags && pnpm publish",
    "test": "pnpm lint && vitest run --coverage"
  },
  "dependencies": {
    "scule": "^1.1.0"
  },
  "peerDependencies": {
    "@subsquid/logger": ">= 1",
    "@subsquid/ss58": ">= 2",
    "@subsquid/substrate-processor": ">= 2",
    "typeorm": "0.3.x"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "latest",
    "@subsquid/logger": "^1.3.1",
    "@subsquid/ss58": "^0.1.4",
    "@subsquid/substrate-processor": "^2.4.1",
    "@vitest/coverage-v8": "latest",
    "changelogen": "^0.5.3",
    "eslint": "latest",
    "typeorm": "^0.3.17",
    "typescript": "latest",
    "unbuild": "latest",
    "vitest": "latest"
  },
  "packageManager": "pnpm@7.9.5",
  "engines": {
    "node": ">=14"
  }
}
